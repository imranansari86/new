<Alloy>
	<View id="requestForOnlineAssist" backgroundColor="#fff" top="0" bottom="0" width="Ti.UI.FILL">
		<View id="topView" height="Ti.UI.SIZE" top="0" layout="vertical" width="90%">
			<Label id="headingLbl" textid="online_self_assesment_label" top="30" class="questionLable"></Label>
		</View>
		<View bottom="80" height="Ti.UI.SIZE" layout="vertical"  width="100%">
			<Button id="yesButton" top="10" class="toolsBtn"  titleid="yes" onClick="openClickAssist" onTouchstart="touchStart" onTouchend="touchEnd"></Button>
			<Button id="noButton" top="10" class="toolsBtn"  titleid="no" onClick="openClickAssist" onTouchstart="touchStart" onTouchend="touchEnd"></Button>
			<Button id="mayBeButton" top="10" class="toolsBtn"  titleid="maybe" onClick="openClickAssist" onTouchstart="touchStart" onTouchend="touchEnd"></Button>
			<View id="barLines" layout="horizontal" top="30" height="5" right="20" left="20">
				<View id="one" class="barLinesClass" backgroundColor="#69b086" left="0"></View>
				<View id="two" class="barLinesClass" backgroundColor="#69b086"></View>
				<View id="three" class="barLinesClass" backgroundColor="#3386b7"></View>
			</View>
		</View>
		<View  bottom="0" height="80" >
			<Label class="bottomLable" textid="question2Of3" textAlign="center"></Label>
			<ImageView left="20" backgroundImage="none" image="/images/back_icon.png" onClick="backtoScreen"></ImageView>
		</View>
	</View>
</Alloy>