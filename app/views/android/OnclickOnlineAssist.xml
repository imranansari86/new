<Alloy>
	<View id="onClickOnlineAsst" height="Ti.UI.FILL" width="Ti.UI.FILL" backgroundColor="#fff">
		<View id="topView" height="Ti.UI.SIZE" top="0" layout="vertical">
			<Label id="headingLbl" textid="online_self_assesment_label" top="30" class="questionLable" width="90%"></Label>
			<View top="20dp" right="20dp" left="20dp" layout="vertical" height="Ti.UI.SIZE">
				<View height="1" left="0" right="0" backgroundColor="#3386b7"></View>
				<Label id="headingLbl" top="10" class="yesNoTitle" width="90%"></Label>
				<View height="1" left="0" right="0" top="10" backgroundColor="#3386b7"></View>
			</View>
			<ScrollView top="20dp" height="50%" scrollType="vertical" layout="vertical">
				<Label id="detail"  top="0" class="detailsLabel" width="90%"></Label>
				<Label id="selfAssesLink" top="5" width="90%" text="https://www.responsepsychological.com/assessment" class="yesNoTitle" onClick="openWebLink"></Label>
			</ScrollView>
		</View>
		<View bottom="80" height="Ti.UI.SIZE" layout="vertical">
			<Button id="nextBitton" top="10" class="toolsBtn"  titleid="next" onClick="openFinishAsst" onTouchstart="touchStart" onTouchend="touchEnd"></Button>
			<!-- <Button id="noButton" top="10" class="toolsBtn"  titleid="no"></Button> -->
			<View id="barLines" layout="horizontal" right="20" left="20" top="30" height="5">
				<View id="one" class="barLinesClass" backgroundColor="#69b086" left="0"></View>
				<View id="two" class="barLinesClass" backgroundColor="#69b086"></View>
				<View id="three" class="barLinesClass" backgroundColor="#69b086"></View>
			</View>
		</View>
		<View bottom="0" height="80">
			<Label class="bottomLable" textAlign="center" id="questionNoTitle"></Label>
			<ImageView left="20" backgroundImage="none" image="/images/back_icon.png" onClick="backtoScreen"></ImageView>
		</View>
	</View>
</Alloy>